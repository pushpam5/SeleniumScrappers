from selenium import webdriver
import time
import json
FILE_NAME = file_path

def save(x):
    with open(FILE_NAME,'w') as f:
        json.dump(x,f)

base_url = 'https://www.forgood.co.za'
url = base_url + '/volunteer'
pause_time = 4
DRIVER_PATH = 'chromedriver.exe'
driver = webdriver.Chrome(executable_path=DRIVER_PATH)
driver.get(url)
time.sleep(pause_time)
page_count = 0
links = []
while page_count < 43:
    try:
        print(page_count)
        li = driver.find_elements_by_tag_name('a')
        for j  in li:
            if j.get_attribute('class') == 'au-target ui bottom attached button fg volunteer action':
                links.append(j.get_attribute('href'))
        if page_count == 0:
            button = driver.find_element_by_xpath('/html/body/div[2]/div/router-view[2]/div/div/div/fg-listing/div[2]/div/div/a[7]')
        else:
            button = driver.find_element_by_xpath('/html/body/div[2]/div/router-view[2]/div/div/div/fg-listing/div[2]/div/div/a[8]')
        button.click()
        time.sleep(pause_time)
        page_count += 1
    except Exception as e:
        print(e)
        page_count += 1
print(links)
print(len(links))
