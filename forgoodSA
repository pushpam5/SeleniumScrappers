count = 0
for i in links:
    try:
        driver = webdriver.Chrome(executable_path=DRIVER_PATH)
        driver.get(i)
        time.sleep(pause_time)
        try:
            hea = driver.find_element_by_xpath('/html/body/div[2]/div/router-view[2]/profile/div/div[1]/div/div/div/h1')
            print(hea.text)
            oppor.append(hea.text)
        except Exception as e:
            oppor.append("Not Found")
        try:
            to = driver.find_element_by_xpath('/html/body/div[2]/div/router-view[2]/profile/div/div[2]/div/div/div/div[2]/div/div[1]/div/p')
            print(to.text)
            todo.append(to.text)
        except Exception as e:
            todo.append("Not Found")
        try:
            ab = driver.find_element_by_xpath('/html/body/div[2]/div/router-view[2]/profile/div/div[2]/div/div/div/div[2]/div/div[1]/div/div[1]/p')
            print(ab.text)
            about.append(ab.text)
        except Exception as e:
            about.append("Not Found")
        try:
            abc = driver.find_element_by_xpath('/html/body/div[2]/div/router-view[2]/profile/div/div[2]/div/div/div/div[2]/div/div[1]/div/div[3]/div/div[2]/a')
            print(abc.get_attribute('href'))
            print(abc.text)
            org_link.append(abc.get_attribute('href'))
            org_name.append(abc.text)
        except Exception as e:
            org_link.append("Not Found")
            org_name.append("Not Found")
        count += 1
        print(count)
        driver.quit()
    except Exception as e:
        print(e)
        break
length = len(org_link)
final_li = []
for i in range(length):
    val = {}
    val['opportunity'] = oppor[i]
    val['what-to-do'] = todo[i]
    val['about'] = about[i]
    val['organisation'] = org_name[i]
    val['org_link'] = org_link[i]
    final_li.append(val)
save(final_li)
print(final_li)
